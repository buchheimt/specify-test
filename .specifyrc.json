{
  "repository": "@thirty-madison/Launch-Design-System",
  "head": "specify",
  "base": "main",
  "rules": [
    {
      "name": "Extract all tokens to JSON",
      "path": "specify_output/all-tokens.json"
    }
  ],
  "parsers": [
    {
      "name": "to-theme-ui",
      "options": {
        "formatTokens": {
          "colorFormat": {
            "format": "hex"
          },
          "fontSizeFormat": {
            "type": "string",
            "unit": "rem"
          }
        }
      }
    }
  ]
}
